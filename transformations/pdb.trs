/* Dataset Basic Information */
transform column "$..'PDBx:entryCategory'.'PDBx:entry'.'@id'" to "dataset.ID" apply {{ result='PDB:' + value }};
transform column "$..'PDBx:struct'.'PDBx:title'.'_$'" to "dataset.title";
transform column "$..'PDBx:struct'.'PDBx:pdbx_descriptor'.'_$'" to "dataset.description";
transform columns "$..'PDBx:struct_keywords'.'PDBx:pdbx_keywords'.'_$'", "$..'PDBx:struct_keywords'.'PDBx:text'.'_$'" to "dataset.keywords[]" apply {{
arr=re.split("\s*,\s*",value1,)
arr.extend(re.split("\s*,\s*",value2))
result=arr
}};

/* Dataset Landing Page*/
transform column "$.'PDBx:datablock'.'@datablockName'.'PDBx:atom_sites'.'@entry_id'" to "access.landingPage" apply {{ result='http://www.rcsb.org/pdb/explore/explore.do?structureId=' + value}};

/* Dataset Creator */
transform column "$.'PDBx:datablock'.'PDBx:citation_authorCategory'.'PDBx:citation_author'[?(@.'@citation_id'='primary')].'@name'" to "dataset.creators[]";

/* Dataset Release Date Information */
transform column "$..'PDBx:database_PDB_revCategory'.'PDBx:database_PDB_rev'[?(@.'@num' = '1')].'PDBx:date'.'_$'" to "dataset.dateReleased" apply toStandardDateTime("yyyy-MM-dd");
transform column "$..'PDBx:database_PDB_revCategory'.'PDBx:database_PDB_rev'[?(@.'@num' = '1')].'PDBx:date_original'.'_$'" to "dataset.dateCreated";

/* Dataset Qualifiers */
let "dataset.availability" = "available";
let "dataset.types[0]" = "structure";
let "dataset.aggregation" = "instance of dataset";
let "dataset.privacy" = "not applicable";
let "dataset.refinement" = "curated";

/* Information on Dataset Distributions */

/* Dataset Distribution - PDB XML */
let "datasetDistribution[0].format" = "PDBML/XML";
transform column "$..'PDBx:entryCategory'.'PDBx:entry'.'@id'" to "datasetDistribution[0].accessURL" apply {{ result='https://files.rcsb.org/download/' + value + '.xml.gz' }};
let "datasetDistribution[0].accessType" = "download";
let "datasetDistribution[0].storedIn" = "Protein Data Bank";
let "datasetDistribution[0].qualifier" = "gzip compressed";
let "datasetDistribution[0].license" = "Data files contained in the PDB archive (ftp://ftp.wwpdb.org) are free of all copyright restrictions and made fully and freely available for both non-commercial and commercial use. Users of the data should attribute the original authors of that structural data. By using the materials available in the PDB archive, the user agrees to abide by the conditions described in the PDB Advisory Notice.";

/* Dataset Distribution - PDB Format */
let "datasetDistribution[1].format" = "PDB Format";
transform column "$..'PDBx:entryCategory'.'PDBx:entry'.'@id'" to "datasetDistribution[1].accessURL" apply {{ result='https://files.rcsb.org/download/' + value + '.pdb.gz' }};
let "datasetDistribution[1].accessType" = "download";
let "datasetDistribution[1].storedIn" = "Protein Data Bank";
let "datasetDistribution[1].qualifier" = "gzip compressed";
let "datasetDistribution[1].license" = "Data files contained in the PDB archive (ftp://ftp.wwpdb.org) are free of all copyright restrictions and made fully and freely available for both non-commercial and commercial use. Users of the data should attribute the original authors of that structural data. By using the materials available in the PDB archive, the user agrees to abide by the conditions described in the PDB Advisory Notice.";

/* Dataset Distribution - PDB mmCIF */
let "datasetDistribution[2].format" = "PDBx/mmCIF";
transform column "$..'PDBx:entryCategory'.'PDBx:entry'.'@id'" to "datasetDistribution[2].accessURL" apply {{ result='https://files.rcsb.org/download/' + value + '.cif.gz' }};
let "datasetDistribution[2].accessType" = "download";
let "datasetDistribution[2].storedIn" = "Protein Data Bank";
let "datasetDistribution[2].qualifier" = "gzip compressed";
let "datasetDistribution[2].license" = "Data files contained in the PDB archive (ftp://ftp.wwpdb.org) are free of all copyright restrictions and made fully and freely available for both non-commercial and commercial use. Users of the data should attribute the original authors of that structural data. By using the materials available in the PDB archive, the user agrees to abide by the conditions described in the PDB Advisory Notice.";

/* Dataset Distribution - FASTA Sequence */
let "datasetDistribution[3].format" = "FASTA";
transform column "$..'PDBx:entryCategory'.'PDBx:entry'.'@id'" to "datasetDistribution[3].accessURL" apply {{ result='http://www.rcsb.org/pdb/download/downloadFile.do?fileFormat=FASTA&compression=NO&structureId=' + value }};
let "datasetDistribution[3].accessType" = "download";
let "datasetDistribution[3].storedIn" = "Protein Data Bank";
let "datasetDistribution[3].license" = "Data files contained in the PDB archive (ftp://ftp.wwpdb.org) are free of all copyright restrictions and made fully and freely available for both non-commercial and commercial use. Users of the data should attribute the original authors of that structural data. By using the materials available in the PDB archive, the user agrees to abide by the conditions described in the PDB Advisory Notice.";

/* Dataset Distribution - Structure Factors */
let "datasetDistribution[4].format" = "CIF";
transform column "$..'PDBx:entryCategory'.'PDBx:entry'.'@id'" to "datasetDistribution[4].accessURL" apply {{ result='https://files.rcsb.org/download/' + value + '-sf.cif.gz' }};
let "datasetDistribution[4].accessType" = "download";
let "datasetDistribution[4].storedIn" = "Protein Data Bank";
let "datasetDistribution[4].qualifier" = "gzip compressed";
let "datasetDistribution[4].license" = "Data files contained in the PDB archive (ftp://ftp.wwpdb.org) are free of all copyright restrictions and made fully and freely available for both non-commercial and commercial use. Users of the data should attribute the original authors of that structural data. By using the materials available in the PDB archive, the user agrees to abide by the conditions described in the PDB Advisory Notice.";

/* Dataset Distribution - Biological Assembly */
let "datasetDistribution[5].format" = "PDB Format (A)";
transform column "$..'PDBx:entryCategory'.'PDBx:entry'.'@id'" to "datasetDistribution[5].accessURL" apply {{ result='https://files.rcsb.org/download/' + value + '.pdb1.gz' }};
let "datasetDistribution[5].accessType" = "download";
let "datasetDistribution[5].storedIn" = "Protein Data Bank";
let "datasetDistribution[5].qualifier" = "gzip compressed";
let "datasetDistribution[5].license" = "Data files contained in the PDB archive (ftp://ftp.wwpdb.org) are free of all copyright restrictions and made fully and freely available for both non-commercial and commercial use. Users of the data should attribute the original authors of that structural data. By using the materials available in the PDB archive, the user agrees to abide by the conditions described in the PDB Advisory Notice.";

/* Dataset Distribution - JSmol Image */
let "datasetDistribution[6].format" = "JSmol";
transform column "$..'PDBx:entryCategory'.'PDBx:entry'.'@id'" to "datasetDistribution[6].accessURL" apply {{ result='http://www.rcsb.org/pdb/explore/jmol.do?structureId=' + value + '&bionumber=1' }};
let "datasetDistribution[6].accessType" = "view";
let "datasetDistribution[6].storedIn" = "Protein Data Bank";
let "datasetDistribution[6].license" = "Data files contained in the PDB archive (ftp://ftp.wwpdb.org) are free of all copyright restrictions and made fully and freely available for both non-commercial and commercial use. Users of the data should attribute the original authors of that structural data. By using the materials available in the PDB archive, the user agrees to abide by the conditions described in the PDB Advisory Notice.";


/* Method Information */
transform column "$..'PDBx:exptlCategory'.'PDBx:exptl'.'@method'" to "dataAcquisition.title";

/* Publication Information */
transform column "$..'PDBx:citation'[?(@.'@id' = 'primary')].'PDBx:pdbx_database_id_PubMed'.'_$'" to "primaryPublications[0].ID" apply {{ result = 'pmid:' + value }};
transform column "$..'PDBx:citation'[?(@.'@id' = 'primary')].'PDBx:pdbx_database_id_DOI'.'_$'" to "primaryPublications[0].alternateID" apply {{ result = 'doi:' + value }};
transform column "$..'PDBx:citation'[?(@.'@id' = 'primary')].'PDBx:title'.'_$'" to "primaryPublications[0].title";
transform column "$..'PDBx:citation'[?(@.'@id' = 'primary')].'PDBx:year'.'_$'" to "primaryPublications[0].dates";
transform column "$.'PDBx:datablock'.'PDBx:citation_authorCategory'.'PDBx:citation_author'[?(@.'@citation_id'='primary')].'@name'" to "primaryPublications[0].authors[]";

/* Identifier Information */
transform columns "$.'PDBx:datablock'.'PDBx:database_2Category'.'PDBx:database_2'[*].'@database_id'",
 "$.'PDBx:datablock'.'PDBx:database_2Category'.'PDBx:database_2'[*].'@database_code'"  to "identifiers[].ID" apply {{
result = value1.lower() + ':' + value2
}};

/* Access Information */
let "access.accesstypes" = "download";
let "access.authorization" = "none";
let "access.authentication[0]" = "none"; 
let "access.authentication[1]" = "simpleLogin";

/* Specific Information on Extended Attributes */
/* Material Information */
transform column "$.'PDBx:datablock'.'PDBx:chem_compCategory'.'PDBx:chem_comp'[*].'PDBx:name'.'_$'" to "material[].name";
transform column "$.'PDBx:datablock'.'PDBx:chem_compCategory'.'PDBx:chem_comp'[*].'PDBx:formula'.'_$'" to "material[].formula";
transform column "$.'PDBx:datablock'.'PDBx:chem_compCategory'.'PDBx:chem_comp'[*].'PDBx:name'.'_$'" to "material[].roles" apply {{ result = 'chemical component' }};

/* Taxonomic Information */
transform union "$.'PDBx:datablock'.'PDBx:entity_src_genCategory'.'PDBx:entity_src_gen'[*].'PDBx:pdbx_gene_src_ncbi_taxonomy_id'.'_$'", "$.'PDBx:datablock'.'PDBx:entity_src_genCategory'.'PDBx:entity_src_gen'[*].'PDBx:pdbx_host_org_ncbi_taxonomy_id'.'_$'" to "taxonomicInformation[].ID" apply {{ result = 'ncbitax:' + value }};
transform union "$.'PDBx:datablock'.'PDBx:entity_src_genCategory'.'PDBx:entity_src_gen'[*].'PDBx:pdbx_gene_src_scientific_name'.'_$'", "$.'PDBx:datablock'.'PDBx:entity_src_genCategory'.'PDBx:entity_src_gen'[*].'PDBx:pdbx_host_org_scientific_name'.'_$'" to "taxonomicInformation[].name";
transform union "$.'PDBx:datablock'.'PDBx:entity_src_genCategory'.'PDBx:entity_src_gen'[*].'PDBx:gene_src_species'.'_$'", "$.'PDBx:datablock'.'PDBx:entity_src_genCategory'.'PDBx:entity_src_gen'[*].'PDBx:host_org_species'.'_$'" to "taxonomicInformation[].species";
transform union "$.'PDBx:datablock'.'PDBx:entity_src_genCategory'.'PDBx:entity_src_gen'[*].'PDBx:gene_src_strain'.'_$'", "$.'PDBx:datablock'.'PDBx:entity_src_genCategory'.'PDBx:entity_src_gen'[*].'PDBx:pdbx_host_org_strain'.'_$'" to "taxonomicInformation[].strain";

/* Gene Information */
transform column "$.'PDBx:datablock'.'PDBx:entity_src_genCategory'.'PDBx:entity_src_gen'[*].'PDBx:pdbx_gene_src_gene'.'_$'" to "gene[].name";

/* Data Repository Information */
let "dataRepository.ID" = "SCR:012820";
let "dataRepository.name" = "Protein Data Bank";
let "dataRepository.abbreviation" = "PDB";
let "dataRepository.homePage"= "http://www.rcsb.org/pdb/";

/* Organization Information */
let "organization.ID" = "SCR:006555";
let "organization.name" = "Worldwide Protein Data Bank";
let "organization.abbreviation" = "wwPDB";
let "organization.homePage" = "http://www.wwpdb.org";


/* Items not included 
transform column "$..'PDBx:citation'[?(@.'@id' = 'primary')].'PDBx:journal_abbrev'.'_$'" to "publication.journal";
transform column "$..'PDBx:citation'[?(@.'@id' = 'primary')].'PDBx:journal_id_ISSN'.'_$'" to "publication.journalISSN";
transform column "$..'PDBx:citation'[?(@.'@id' = 'primary')].'PDBx:page_first'.'_$'" to "publication.firstPage";
transform column "$..'PDBx:citation'[?(@.'@id' = 'primary')].'PDBx:page_last'.'_$'" to "publication.lastPage";
transform column "$..'PDBx:citation'[?(@.'@id' = 'primary')].'PDBx:pdbx_database_id_DOI'.'_$'" to "citation.count" apply {{ result = '0' }};

transform column "$.'PDBx:datablock'.'PDBx:chem_compCategory'.'PDBx:chem_comp'[*].'PDBx:formula_weight'.'_$'" to "material[].weight";
transform column "$.'PDBx:datablock'.'PDBx:chem_compCategory'.'PDBx:chem_comp'[*].'PDBx:type'.'_$'" to "material[].type";

transform union "$.'PDBx:datablock'.'PDBx:entity_src_genCategory'.'PDBx:entity_src_gen'[*].'PDBx:gene_src_genus'.'_$'", "$.'PDBx:datablock'.'PDBx:entity_src_genCategory'.'PDBx:entity_src_gen'[*].'PDBx:host_org_genus'.'_$'" to "taxonomicInformation[].genus";
transform union "$.'PDBx:datablock'.'PDBx:entity_src_genCategory'.'PDBx:entity_src_gen'[*].'PDBx:pdbx_gene_src_common_name'.'_$'", "$.'PDBx:datablock'.'PDBx:entity_src_genCategory'.'PDBx:entity_src_gen'[*].'PDBx:pdbx_host_org_common_name'.'_$'" to "taxonomicInformation[].name";
*/
