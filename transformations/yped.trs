/* Dataset Information */
transform column "$.'project_name'" to "dataset.id";
transform column "$.'project_name'" to "dataset.title";
transform column "$.'description'" to "dataset.description";

/* Dataset Distribution */
transform column "$.'samples'" to "datasetdistribution.size";

/* Data Repository Information */
let "dataRepository.ID" = "SCR:001436";
let "dataRepository.name" = "Yale Protein Expression Database";
let "dataRepository.abbreviation" = "YPED";
let "dataRepository.homePage" = "http://medicine.yale.edu/keck/nida/yped.aspx";

/* Publication Information */
transform column "$.'pubmedid'" to "publication.ID" apply {{ result = 'pmid:' + value }};

/* Access Information */
transform column "$.'project_url'" to "access.landingPage" apply {{ result = 'http://yped.med.yale.edu/repository/' + value }};
let "access.accesstypes" = "download";
let "access.authorization" = "none";
let "access.authentication" = "none";

/* Person Information */
transform column "$.'pi'" to "person.name";

/* Organization Information */
let "organization.ID" = "SCR:006339";
let "organization.name" = "Yale School of Medicine";
let "organization.abbreviation" = "Yale";
let "organization.homePage" = "http://medicine.yale.edu/index.aspx";

/* Taxonomic Information */
transform column "$.'organisms'" to "taxonomicinformation.name";

/* Data Acquisition */
transform column "$.'analysis_type'" to "dataAcquisition.name";

/* Items not included */
