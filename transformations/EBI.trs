}};


/* Data Acquisition */


transform column "$.'run'.'Overview'.'Other Information'.'collection date'" to "DataAcquisition.startDate[0]";
transform column "$.'run'.'Overview'.'Other Information'.'collection time'" to "DataAcquisition.startDate[1]";




/* Data Repository */ 


transform column "$.'run'.'Overview'.'Other Information'.'ENA checklist'" to "DataRepository.name";




/* Instrument */


transform column "$.'run'.'Overview'.'Other Information'.'instrument model'" to "Instrument.name";