/* Dataset */


transform column "$.'run'.'Downloads'.'Sequence Data'.'Predicted CDS'" to "Dataset.downloadURL[0].URL";
transform column "$.'run'" to "Dataset.downloadURL[0].name" apply {{
result = 'Predicted CDS'
}};
