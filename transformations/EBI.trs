/* Organism */


transform column "$.'run'.'Overview'.'Other Information'.'host common name'" to "Organism.name";


transform column "$.'run'.'Overview'.'Other Information'.'anonymized name'" to "Organism.identifier[0].value";
transform column "$.'run'" to "Organism.identifier[0].type" apply {{
result = 'anonymized name'
}};
transform column "$.'run'.'Overview'.'Other Information'.'host subject id'" to "Organism.identifier[1].value";
transform column "$.'run'" to "Organism.identifier[1].type" apply {{
