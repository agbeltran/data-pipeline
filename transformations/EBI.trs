/* Data Analysis */


transform column "$.'run'.'Overview'.'Data Analysis'.'Analysis date:'" to "Data Analysis.startDate";


transform column "$.'run'.'Overview'.'Data Analysis'.'Experiment type:'" to "Study.studyType";


transform column "$.'run'.'Overview'.'Data Analysis'.'Pipeline version:'" to "Data Analysis.uses";




/* Material */


transform column "$.'run'.'Overview'.'Description'.'Classification:'" to "Material.derivesFrom[0].value";
transform column "$.'run'" to "Material.derivesFrom[0].name" apply {{
result = 'classification'
}};
transform column "$.'run'.'Overview'.'Description'.'Sample name:'" to "Material.derivesFrom[1].value";
transform column "$.'run'" to "Material.derivesFrom[1].name" apply {{
result = 'sample name'
}};
transform column "$.'run'.'Overview'.'Other Information'.'body habitat'" to "Material.derivesFrom[2].value";
transform column "$.'run'" to "Material.derivesFrom[2].name" apply {{
result = 'body habitat'
}};
transform column "$.'run'.'Overview'.'Other Information'.'body product'" to "Material.derivesFrom[3].value";
transform column "$.'run'" to "Material.derivesFrom[3].name" apply {{
result = 'body product'
}};
transform column "$.'run'.'Overview'.'Other Information'.'body site'" to "Material.derivesFrom[4].value";
transform column "$.'run'" to "Material.derivesFrom[4].name" apply {{
result = 'body site'
}};
transform column "$.'run'.'Overview'.'Host Associated'.'Species:'" to 
"Material.derivesFromOrganism";


transform column "$.'run'.'Overview'.'Other Information'.'common name'" to "Material.name";


transform column "$.'run'.'Overview'.'Other Information'.'environment (biome)'" to "Material.hasCharacteristic[0].value";
transform column "$.'run''" to "Material.hasCharacteristic[0].name" apply {{
result = 'biome'
}};
transform column "$.'run'.'Overview'.'Other Information'.'environment (feature)'" to "Material.hasCharacteristic[1].value";
transform column "$.'run''" to "Material.hasCharacteristic[1].name" apply {{
result = 'feature'
}};
transform column "$.'run'.'Overview'.'Other Information'.'environment (material)'" to "Material.hasCharacteristic[2].value";
transform column "$.'run''" to "Material.hasCharacteristic[2].name" apply {{
result = 'material'
}};
transform column "$.'run'.'Overview'.'Other Information'.'NCBI sample classification'" to "Material.identifier";




/* Study */


transform column "$.'run'.'Overview'.'Description'.'Project name:'" to "Study.title";




/* Activity */ 


transform column "$.'run'.'Overview'.'Title'" to "Activity.title";


transform column "$.'run'.'Overview'.'Other Information'.'altitude'" to "Activity.location[0].value";
transform column "$.'run'" to "Activity.location[0].name" apply {{
