/* Dataset Information */
transform column "$.'id'" to "dataset.ID";
transform column "$.'title'" to "dataset.title";
transform column "$.'description'" to "dataset.description";

/* Data Repository Information */
let "dataRepository.name" = "OpenfMRI";
let "dataRepository.abbreviation" = "OpenfMRI";
let "dataRepository.ID" apply = "SCR:005031";
let "dataRepository.homePage" = "http://openfmri.org/";

/* Access Information */
transform column "$.'link'" to "access.landingPage";
let "access.accesstypes" = "download";
let "access.authorization" = "none";
let "access[0].authentication" = "none"; 
let "access[1].authentication" = "simpleLogin"; 

/* Organization Information */
let "organization.name" = "Stanford University";
let "organization.abbreviation" = "Stanford";
let "organization.ID" = "SCR:011538";
let "organization.homePage" = "http://www.stanford.edu/";

/* Taxonomic Information */
transform column "$.'organism'" to "taxonomicInformation[0].name";

/* Data Acquisition */
transform column "$.'assay'" to "dataAcquisition.name";

