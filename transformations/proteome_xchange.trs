/* Dataset Information */
transform column "$.'dataset_identifier'" to "dataset.ID";
transform column "$.'title'" to "dataset.title";
transform column "$.'keywords'" to "dataset.keywords[]" apply {{
arr=re.split("\s*;\s*",value,)
result=arr
}};

/* Dataset Distribution */
transform column "$.'announcement_date'" to "datasetdistribution.dateReleased" apply toStandardDateTime("yyyy-MM-dd");

/* Data Repository Information */
let "dataRepository.ID" = "SCR:004055";
let "dataRepository.name" = "ProteomeXchange";
let "dataRepository.abbreviation" = "ProteomeXchange";
let "dataRepository.homePage" = "http://www.proteomexchange.org/";

/* Publication Information */
transform column "$.'publication'" to "publication.title";

/* Access Information */
transform column "$.'dataset_url'" to "access.landingPage";
let "access.accesstypes" = "download";
let "access.authorization" = "none";
let "access.authentication" = "none";

/* Person Information */
transform column "$.'primary_submitter'" to "person[0].name";
transform column "$.'labhead'" to "person[1].name";

/* Organization Information */
let "organization.ID"  = "SCR:004727";
let "organization.name"  = "European Bioinformatics Institute";
let "organization.abbreviation" = "EBI";
let "organization.homePage" = "http://www.ebi.ac.uk/";

/* Taxonomic Information */
transform column "$.'species'" to "taxonomicInformation[0].name";

/* Instrument information */
transform column "$.'instrument'" to "instrument.name";

/* Items not included */
