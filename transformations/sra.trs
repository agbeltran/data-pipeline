transform column "$.'SRA'.'RUN_SET'.'RUN'[*].'@run_center'" to "SRA.RUN_SET.RUN[].@run_center";
transform column "$.'SRA'.'SUBMISSION'.'@alias'" to "SRA.SUBMISSION.@alias";
