transform column "$.'Database Description'.'General information of database'.'Reference(s)''.External Links'" to "Publication.extraProperties[].External Links[]";
transform column "$.'Database Description'.'General information of database'.'Database classification'[*]" to "DataType.information[]";
transform column "$.'Database Description'.'General information of database'.'Contact address'" to "Organization.relatedIdentifiers";
transform column "$.'Database Description'.'Original website information'.'Query search'" to "Dataset.extraProperties[].Query search";
transform column "$.'Database Description'.'Original website information'.'Entry list'" to "Dataset.extraProperties[].Entry list";
transform column "$.'Database Description'.'Original website information'.'Operation start date'" to "Dataset.dates[].Operation start date";
transform column "$.'Database Description'.'Original website information'.'Last update date'" to "Dataset.dates[].Last update date";
transform column "$.'Database Description'.'Original website information'.'URL of the portal site'" to "Access.landingPage[]";
transform column "$.'Database Description'.'Original website information'.'Database maintenance site'" to "Access.landingPage[]";
transform column "$.'Database Description'.'Original website information'.'URL of the original website'" to "Access.accessURL[]";
transform column "$.'Database Description'.'Original website information'.'Whole data download'" to "Access.accessURL[]";
