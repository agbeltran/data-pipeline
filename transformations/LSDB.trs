transform column "$.'Database Description'.'General information of database'.'Database name'" to "Dataset.title";
transform column "$.'Database Description'.'General information of database'.'DOI'" to "Dataset.identifiers";
transform column "$.'Database Description'.'General information of database'.'Database classification'[*]" to "Dataset.types[]";
transform column "$.'Database Description'.'General information of database'.'Creator'[*].'Creator Name'" to "Person[].fullName";
transform column "$.'Database Description'.'General information of database'.'Creator'[*].'Creator Name'" to "Person[].role" apply {{result="creator"}};
transform column "$.'Database Description'.'General information of database'.'Creator'[*].'Creator Affiliation'" to "Person[].affiliations";
transform column "$.'Database Description'.'General information of database'.'Database description'" to "Dataset.description";
transform column "$.'Database Description'.'General information of database'.'Organism'[*].'Taxonomy Name'" to "BiologicalEntity[].name";
transform column "$.'Database Description'.'General information of database'.'Organism'[*].'Taxonomy ID'" to "BiologicalEntity[].identifiers";
transform column "$.'Database Description'.'General information of database'.'Organism'[*].'Taxonomy ID'" to "TaxonomicInformation[].name" apply {{result="NCBI Taxonomy"}};
transform column "$.'Database Description'.'General information of database'.'Features and manner of utilization of database'" to "Dataset.extraProperties[].Features and manner of utilization of database";
transform column "$.'Database Description'.'General information of database'.'Background and funding'.'name'[*]" to "Grant.name[]";
transform column "$.'Database Description'.'General information of database'.'Reference(s)'.'Article title'" to "Publication.title";
transform column "$.'Database Description'.'General information of database'.'Reference(s)'.'Author name(s)'" to "Publication.authorsList";
