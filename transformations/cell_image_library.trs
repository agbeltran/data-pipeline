/* Dataset Information */
transform column "$.'id'" to "dataset.ID";
transform column "$.'id'" to "dataset.title" apply {{ result = 'Cell Image Library Dataset CIL:' + value }};
transform column "$.'imagedescription'" to "dataset.description";
transform column "$.'parameterimaged'" to "dataset.dimensions";
transform column "$.'itemtype'" to "dataset.types";

/* Data Repository Information */
let "dataRepository.ID" = "SCR:003510";
let "dataRepository.name" = "Cell Image Library";
let "dataRepository.abbreviation" = "CIL";
let "dataRepository.homePage" = "http://www.cellimagelibrary.org/";

/* Access Information */
transform column "$.'id'" to "access.landingPage" apply {{ result = 'http://www.cellimagelibrary.org/images/' + value }};
let "access.accesstypes"  = "download";
let "access.authorization"  = "none";
let "access.authentication" = "none";

/* Organization Information */
let "organization.ID" = "SCR:010600";
let "organization.name" = "American Society for Cell Biology";
let "organization.abbreviation" = "ASCB";
let "organization.homePage" = "http://ascb.org/";

/* Taxonomic Information */
transform column "$.'ncbiorganismalclassification'" to "taxonomicinforamtion.name[]" apply {{
arr=re.split("\s*,\s*",value1,)
arr.extend(re.split("\s*,\s*",value2))
result=arr
}};

/* Treatment */
transform column "$.'preparation'" to "treatment.name";
transform column "$.'visualizationmethods'" to "treatment.agent";

/* Data Acquisition Information */
transform column "$.'imagingmode'" to "dataAcquisition.name";

/* Biological Entity */
transform column "$.'celltype'" to "BiologicalEntity[].name" apply {{
arr=re.split("\s*,\s*",value1,)
arr.extend(re.split("\s*,\s*",value2))
result=arr
}};

